<h2>商品一覧</h2>

<%= link_to "+", new_admin_item_path %>

<% @items.each do |items| %>
  
<div>
  <table>
    <thead>
      <tr>
        <th>商品ID</th>
        <th>商品名</th>
        <th>税抜価格</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><%= items.id %></td>
        <td><%= items.name %></td>
        <td><%= items.price %></td>
      </tr>
    </tbody>
  </table>
</div>
<% end %>